class nivelusuarioservice
import Usuario;
import NivelUsuario;
import java.util.List;

	public interface NivelUsuarioService{

		public void alterarNivelUsuario(Usuario usuario, NivelUsuario nivelUsuario);

		public void deletarNivelUsuario(Usuario usuario, NivelUsuario nivelUsuario);

		public void inserirNivelUsuario(Usuario usuario, NivelUsuario nivelUsuario);

		public List<NivelUsuario> consultarNivelUsuario(Usuario usuario,NivelUsuario nivelUsuario);

}
